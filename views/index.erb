<ul>
  <% @list.each do |bookmark| %>
    <li>
      <a href="<%= bookmark.url %>">
        <%= bookmark.title %>
      </a>
      <form action='/delete' method='POST'>
      <input type="submit" name='delete' id="<%= bookmark.id %>">
      </form>
    </li>
  <% end %>
</ul>

<form action='/bookmarks/new'>
  <input type="submit" name="add" value="add">
</form>
